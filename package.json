{
  "name": "oldcord",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "workspaces": [
    "frontend/*",
    "oldplunger"
  ],
  "scripts": {
    "build": "npm run build:selector && npm run build:admin && npm run build:oldplunger && npm run build:server",
    "build:admin": "npx rimraf www_static/assets/admin && npm run build --workspace=frontend/admin",
    "build:oldplunger": "npx rimraf www_static/assets/oldplunger && npm run build --workspace=oldplunger",
    "build:selector": "npx rimraf www_static/assets/selector && npm run build --workspace=frontend/selector",
    "build:server": "npx rimraf dist_server && tsc -p server",
    "dev:admin": "npm run dev --workspace=frontend/admin",
    "dev:oldplunger": "npm run dev --workspace=oldplunger",
    "dev:selector": "npm run dev --workspace=frontend/selector",
    "dev:server": "tsx server/index.ts",
    "eslint": "npx eslint .",
    "eslint:fix": "npx eslint --fix .",
    "lint": "run-s -c eslint prettier",
    "lint:fix": "run-s -c eslint:fix prettier",
    "prepare": "husky",
    "prettier": "npx prettier . --write",
    "start": "npm run build && npm run start:server",
    "start:server": "npm run build:server && node dist_server/index.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@sapphire/snowflake": "^3.5.5",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "jimp": "^1.6.0",
    "libsodium-wrappers": "^0.8.1",
    "mediasoup": "^3.19.14",
    "multer": "^2.0.2",
    "murmurhash-js": "^1.0.0",
    "pg": "^8.17.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "react-transition-group": "^4.4.5",
    "rimraf": "^6.1.2",
    "semantic-sdp": "^3.31.1",
    "speakeasy": "^2.0.0",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-svgr": "^4.5.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/express": "^5.0.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "3.8.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "optionalDependencies": {
    "erlpack": "^0.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.1"
  }
}
